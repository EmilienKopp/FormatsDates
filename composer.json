{
  "name": "emilienkopp/formats-dates-trait",
  "description": "A Laravel-Carbon trait for easy date formatting",
  "type": "library",
  "license": "MIT",
  "authors": [
      {
          "name": "Emilien Kopp",
          "email": "emilien.kopp@gmail.com"
      }
  ],
  "require": {
      "php": "^8.2",
      "nesbot/carbon": "^2.0",
      "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.0",
      "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "autoload": {
      "psr-4": {
          "EmilienKopp\\DatesFormatter\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "EmilienKopp\\Tests\\": "Tests/"
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "sort-packages": true
  }
}